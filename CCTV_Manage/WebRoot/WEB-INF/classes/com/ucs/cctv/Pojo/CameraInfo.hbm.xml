<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ucs.cctv.Pojo">
    <class name="CameraInfo" table="camera_info" catalog="cctv_manage">
        <id name="cameraId" type="java.lang.Integer">
            <column name="camera_id" />
            <generator class="identity" />
        </id>
        <property name="cameraName" type="string">
            <column name="camera_Name" length="50" />
        </property>
        <property name="cameraModel" type="string">
            <column name="camera_Model" length="20" />
        </property>
        <property name="cameraIp" type="string">
            <column name="camera_IP" length="20" />
        </property>
        <property name="cameraMac" type="string">
            <column name="camera_Mac" length="50" />
        </property>
        <property name="capacity" type="java.lang.Integer">
            <column name="capacity" />
        </property>
        <property name="section" type="string">
            <column name="section" length="50" />
        </property>
        <property name="cameraType" type="string">
            <column name="camera_Type" length="20" />
        </property>
        <set name ="monitorGroups" table="camera_group" cascade="save-update" lazy="false">
            <key column="camera_id"/>
            <many-to-many column="group_id" class="MonitorGroup"/>
        </set>
    </class>
</hibernate-mapping>
