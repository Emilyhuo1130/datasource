<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  

	
	<filter>
    <filter-name>msgs</filter-name>
    <filter-class>org.nutz.mvc.NutFilter</filter-class>
    <init-param>
        <param-name>skip-mode</param-name>
        <param-value>true</param-value>
    </init-param>
	</filter>
	<filter-mapping>
	    <filter-name>msgs</filter-name>
	    <url-pattern>*.jsp</url-pattern>
	</filter-mapping>
	
	
	<servlet>
    <servlet-name>nutz</servlet-name>
        <servlet-class>org.nutz.mvc.NutServlet</servlet-class>
        <init-param>
            <param-name>modules</param-name>
            <param-value>com.mvcTest.LogServer</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
	    <servlet-name>nutz</servlet-name>
	    <url-pattern>*.nut</url-pattern>
	</servlet-mapping>
	
</web-app>
